# Development environment configuration

# Model Configuration
RERANKER_MODEL=BAAI/bge-reranker-base
RERANKER_MAX_LENGTH=512
RERANKER_QUANTIZATION=none
RERANKER_LOCAL_MODEL_PATH=

# Performance Settings  
RERANKER_MAX_PARALLEL=2
RERANKER_MAX_QUEUE=5
RERANKER_QUEUE_TIMEOUT=30
RERANKER_BATCH_SIZE=8
RERANKER_WORKER_TIMEOUT=60

# Device Configuration
RERANKER_DEVICE=auto
RERANKER_USE_MLX=false

# Caching
ENABLE_PREDICTION_CACHE=true
CACHE_TTL_SECONDS=300
CLEAR_CACHE_INTERVAL=1800

# Redis (disabled for development)
REDIS_ENABLED=false
REDIS_URL=redis://localhost:6379/0

# Advanced Features (disabled for development)
MICRO_BATCH_ENABLED=false
RERANKER_ENABLE_DISTRIBUTED=false

# Server Configuration
HOST=127.0.0.1
PORT=8000
LOG_LEVEL=DEBUG

# Performance Optimizations (disabled for development)
WARMUP_ON_START=false
ENABLE_TORCH_COMPILE=false
ENABLE_MIXED_PRECISION=false